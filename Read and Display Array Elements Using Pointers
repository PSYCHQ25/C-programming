#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];
    int *ptr = arr;   

    
    for (int i = 0; i < N; i++) {
        scanf("%d", (ptr + i));
    }

    
    for (int i = 0; i < N; i++) {
        printf("%d ", *(ptr + i));
    }

    return 0;
}

