#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    int N;
    unsigned long long factorial = 1;  

    scanf("%d", &N);

    
    if (N < 0) {
        printf("Factorial not defined for negative numbers\n");
        return 0;
    }

    for (int i = 1; i <= N; i++) {
        factorial *= i;
    }

    printf("%llu\n", factorial);

    return 0;
}
    
    
