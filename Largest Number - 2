#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    int A, B, C;

    
    
    if (scanf("%d %d %d", &A, &B, &C) != 3) {
        
        return 1;
    }

    
    if (A >= B && A >= C) {
        
        printf("%d\n", A);
    } else if (B >= A && B >= C) {
        
        
        printf("%d\n", B);
    } else {
        
        printf("%d\n", C);
    }


    return 0;
}
  

