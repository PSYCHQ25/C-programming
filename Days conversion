#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int total_days;

    
    int years, weeks, days;

    
    scanf("%d", &total_days);

    
    
    years = total_days / 365;

    
    int remaining_days = total_days % 365;

  
    weeks = remaining_days / 7;

    
    days = remaining_days % 7;

   
    printf("Years: %d\n", years);
    printf("Weeks: %d\n", weeks);
    printf("Days: %d\n", days);



   
    return 0;
}
