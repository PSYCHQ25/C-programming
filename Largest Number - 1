#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    int A, B;

    
    if (scanf("%d %d", &A, &B) != 2) {
        
        return 1;
    }

   
    if (A >= B) {
        
        printf("%d\n", A);
    } else {
        
        printf("%d\n", B);
    }

    return 0;
}   
   
