#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int sumOfDigits(int num) {
    
    if (num == 0)
        return 0;
    
    return (num % 10) + sumOfDigits(num / 10);
}

int main() {
    int n;
    scanf("%d", &n);
    printf("%d", sumOfDigits(n));
    return 0;
}
