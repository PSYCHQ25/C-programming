#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    char ch;

    
    if (scanf(" %c", &ch) != 1) {
        printf("Invalid Input\n");
        return 1;
    }

    
    
    if (!((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z'))) {
        printf("Invalid Input\n");
        return 0; 
    }

  
    
    switch (ch) {
     
        case 'A':
        case 'E':
        case 'I':
        case 'O':
        case 'U':
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
            printf("Vowel\n");
            break;
        
        
        default:
            printf("Consonant\n");
            break;
    }

    return 0;
}
