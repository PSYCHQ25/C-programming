#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int R, C;
    scanf("%d %d", &R, &C);

    int arr[R][C];
    long long sum = 0;  

    
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            scanf("%d", &arr[i][j]);
            sum += arr[i][j];
        }
    }

   
    printf("%lld\n", sum);

    return 0;
}
   
  

