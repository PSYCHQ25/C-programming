#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>
#include <string.h>

int main(void) {
    char S[1001];
    if (fgets(S, sizeof(S), stdin) == NULL) {
        return 0;
    }
   
    size_t len = strlen(S);
    if (len > 0 && S[len-1] == '\n') {
        S[len-1] = '\0';
        len--;
    }

    int seen[256] = {0}; 
    char result[1001];
    size_t writeIndex = 0;

    for (size_t i = 0; i < len; i++) {
        unsigned char c = (unsigned char) S[i];
        if (seen[c] == 0) {
            result[writeIndex++] = S[i];
            seen[c] = 1;
        }
        
    }

    result[writeIndex] = '\0';

    printf("%s\n", result);
    return 0;
}
