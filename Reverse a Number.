#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    int N, reversed = 0;
    scanf("%d", &N);

    int isNegative = 0;
    if (N < 0) {
        isNegative = 1;
        N = -N;
    }

    while (N > 0) {
        int digit = N % 10;
        reversed = reversed * 10 + digit;
        N /= 10;
    }

    if (isNegative) {
        reversed = -reversed;
    }

    printf("%d\n", reversed);

    return 0;
}
    
 
