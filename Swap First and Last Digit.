#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>
#include <math.h>

int main() {
    int N;
    scanf("%d", &N);

    int sign = 1;
    if (N < 0) {
        sign = -1;
        N = -N;
    }

    
    if (N >= 0 && N < 10) {
        printf("%d\n", sign * N);
        return 0;
    }

    int lastDigit = N % 10;

    
    int temp = N;
    int digits = 0;
    while (temp > 0) {
        temp /= 10;
        digits++;
    }

    int firstDigit = N / (int)pow(10, digits - 1);

    
    int middlePart = N % (int)pow(10, digits - 1);

    
    middlePart = middlePart / 10;

   
    int swappedNumber = lastDigit * (int)pow(10, digits - 1) + middlePart * 10 + firstDigit;

    
    swappedNumber *= sign;

    printf("%d\n", swappedNumber);

    return 0;
}
