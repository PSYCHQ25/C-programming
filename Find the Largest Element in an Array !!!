#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    int N;
    int arr[100]; 

 
    scanf("%d", &N);

    
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    
    int max = arr[0];

    
    for (int i = 1; i < N; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }

   
    printf("%d", max);

    return 0;
}




