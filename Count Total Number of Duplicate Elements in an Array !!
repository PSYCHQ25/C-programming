#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N); 

    int arr[N];
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int duplicateCount = 0;
    int counted[N]; 
    for(int i = 0; i < N; i++) counted[i] = 0;

    for(int i = 0; i < N; i++) {
        if(counted[i] == 0) {
            int count = 1;
            for(int j = i + 1; j < N; j++) {
                if(arr[j] == arr[i]) {
                    count++;
                    counted[j] = 1; 
                }
            }
            if(count > 1) {
                duplicateCount++;
            }
        }
    }

    printf("%d\n", duplicateCount);

    return 0;
}
