#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int largest = arr[0];
    int secondLargest = -10001; 

    
    for(int i = 1; i < N; i++) {
        if(arr[i] > largest) {
            largest = arr[i];
        }
    }

    
    for(int i = 0; i < N; i++) {
        if(arr[i] < largest && arr[i] > secondLargest) {
            secondLargest = arr[i];
        }
    }

    if(secondLargest == -10001) {
        printf("No second largest element\n");
    } else {
        printf("%d\n", secondLargest);
    }

    return 0;
}
