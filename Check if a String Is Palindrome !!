#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char S[1001];
    if (fgets(S, sizeof(S), stdin) == NULL) {
        return 0;
    }
    
    size_t len = strlen(S);
    if (len > 0 && S[len-1] == '\n') {
        S[len-1] = '\0';
        len--;
    }

    
    char cleaned[1001];
    size_t j = 0;
    for (size_t i = 0; i < len; i++) {
        if (S[i] != ' ') {
            cleaned[j++] = tolower((unsigned char)S[i]);
        }
    }
    cleaned[j] = '\0';

    
    size_t left = 0, right = (j > 0 ? j-1 : 0);
    int isPal = 1;  
    while (left < right) {
        if (cleaned[left] != cleaned[right]) {
            isPal = 0;
            break;
        }
        left++;
        right--;
    }

    if (isPal)
        printf("Palindrome\n");
    else
        printf("Not Palindrome\n");

    return 0;
}
