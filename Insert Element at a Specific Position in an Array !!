#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N); 

    int arr[101]; 
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]); 
    }

    int value, position;
    scanf("%d", &value);      
    scanf("%d", &position);   

    
    for(int i = N; i >= position; i--) {
        arr[i] = arr[i-1];
    }

    
    arr[position - 1] = value;
    N++; 

    
    for(int i = 0; i < N; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
 


