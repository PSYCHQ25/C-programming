#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int R, C;
    int arr[100][100]; 

    
    scanf("%d %d", &R, &C);

    
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            scanf("%d", &arr[i][j]);
        }
    }

    
    for (int i = 0; i < R; i++) {
        int row_sum = 0;
        for (int j = 0; j < C; j++) {
            row_sum += arr[i][j];
        }
        printf("%d\n", row_sum);
    }

    
    for (int j = 0; j < C; j++) {
        int col_sum = 0;
        for (int i = 0; i < R; i++) {
            col_sum += arr[i][j];
        }
        printf("%d\n", col_sum);
    }

    return 0;
}

