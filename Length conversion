#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    
    int cm;
    
    
    double meters, kilometers;

    
    scanf("%d", &cm);

    

    
    meters = (double)cm / 100.0;
    
    
    kilometers = (double)cm / 100000.0;

  
    printf("%.2f %.2f\n", meters, kilometers);

    return 0;
}
   
   
