#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>
#include <string.h>

int main() {
    char S[1001];              
    if (fgets(S, sizeof(S), stdin) == NULL) {
        return 0;
    }
    
    size_t len = strlen(S);
    if (len > 0 && S[len-1] == '\n') {
        S[len-1] = '\0';
        len--;
    }

   
    size_t i = 0, j = len > 0 ? len-1 : 0;
    while (i < j) {
        char tmp = S[i];
        S[i] = S[j];
        S[j] = tmp;
        i++;
        j--;
    }

    printf("%s\n", S);
    return 0;
}

